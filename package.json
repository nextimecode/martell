{
  "name": "martell",
  "version": "0.0.0-development",
  "description": "Insubmissos, imbatíveis, inquebráveis: este projeto inspirado na Casa Nymeros Martell de Lançassolar traz a essência da exclusividade e resiliência de Dorne. Uma plataforma única que une conteúdo premium e inovação tecnológica, projetada para atender aos mais exigentes padrões de qualidade e segurança. O sol de Dorne brilha intensamente aqui!",
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": "22.12.0"
  },
  "private": true,
  "keywords": [],
  "author": "NeXTIME",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "vercel-build": "pnpm install && pnpm build",
    "build": "prisma generate && turbo run build --parallel",
    "clean:all": "turbo run clean && pnpm store prune && rm -rf .next .turbo node_modules tsconfig.tsbuildinfo pnpm-lock.yaml && pnpm i && pnpm build",
    "coverage": "turbo run coverage",
    "e2e": "turbo run e2e",
    "test": "turbo run test",
    "dev": "prisma generate && turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint --parallel",
    "audit": "pnpm audit",
    "outdated": "pnpm outdated",
    "release": "semantic-release",
    "semantic-release:ui": "semantic-release --config semantic-release-ui.config.js",
    "release:ui": "turbo run build --filter=ui && changeset publish",
    "reset:all": "pnpm clean:all && pnpm i && turbo run build --parallel",
    "start": "turbo run start",
    "typecheck": "turbo run typecheck --parallel",
    "update": "pnpm update --latest",
    "oberyn:build": "turbo run build --filter oberyn",
    "oberyn:dev": "turbo run dev --filter oberyn",
    "oberyn:e2e": "turbo run e2e --filter oberyn",
    "oberyn:e2e:ui": "turbo run e2e:ui --filter oberyn",
    "oberyn:e2e:chromium": "turbo run e2e:chromium --filter oberyn",
    "oberyn:e2e:debug": "turbo run e2e:debug --filter oberyn",
    "oberyn:e2e:codegen": "turbo run e2e:codegen --filter oberyn",
    "oberyn:e2e:report": "turbo run e2e:report --filter oberyn",
    "ui:build": "turbo run build --filter ui",
    "ui:dev": "turbo run dev --filter ui",
    "precommit": "turbo lint typecheck audit",
    "commit": "cz",
    "prepare": "husky",
    "postinstall": "husky",
    "depcheck": "depcheck"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.717.0",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.15",
    "@prisma/client": "6.1.0",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.2",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tailwindcss/forms": "^0.5.9",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.13",
    "eslint-plugin-unicorn": "^56.0.1",
    "framer-motion": "^11.15.0",
    "ioredis": "^5.4.2",
    "lucide-react": "^0.468.0",
    "multer": "1.4.5-lts.1",
    "next": "^15.1.2",
    "passport-jwt": "^4.0.1",
    "preline": "^2.6.0",
    "punycode": "^2.3.1",
    "react": "^19.0.0",
    "react-world-flags": "^1.6.0",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0",
    "zod": "^3.24.1",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@faker-js/faker": "^9.3.0",
    "@nestjs/cli": "^10.4.9",
    "@playwright/test": "^1.49.1",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "22.10.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "19.0.2",
    "@types/react-world-flags": "^1.6.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-helpers": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-perfectionist": "^4.4.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "prisma": "^6.1.0",
    "semantic-release": "^24.2.0",
    "tsup": "^8.3.5",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2",
    "unplugin-swc": "^1.5.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nextimecode/stark.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "overrides": {
    "uri-js": "npm:uri-js-replace"
  }
}
